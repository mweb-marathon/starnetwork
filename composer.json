{
    "require": {
        "php": ">=5.3.0",
        "wordpress": "4.1.1",
        "composer/installers": "~1.0",
        "fancyguy/webroot-installer": "1.0.0",
        "wpackagist-plugin/meta-box": "4.4.2",
        "wpackagist-plugin/events-manager": "5.5.5",
        "wpackagist-plugin/wp-migrate-db": "0.6.1",
        "wpackagist-plugin/simple-share-buttons-adder": "5.5",
        "wpackagist-plugin/taxonomy-terms-order": "1.4.1",
        "wpackagist-plugin/adrotate": "3.11.1",
        "wpackagist-theme/twentyfourteen": "1.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "wordpress",
                "type": "webroot",
                "version": "4.1.1",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/WordPress/WordPress/archive/4.1.1.zip"
                },
                "require": {
                    "fancyguy/webroot-installer": "1.0.0"
                }
            }
        }
    ],
    "extra"       : {
        "webroot-dir"    : "wp",
        "webroot-package": "wordpress"
    }
}